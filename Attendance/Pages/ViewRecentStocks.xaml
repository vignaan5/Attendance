<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Attendance.Pages.ViewRecentStocks"
             Title="ViewRecentStocks">
    <ScrollView Orientation="Vertical">
        
     
        <VerticalStackLayout x:Name="vs" VerticalOptions="Center" HorizontalOptions="Center">

            <DatePicker x:Name="dtpicker" IsEnabled="False" IsVisible="False" DateSelected="dtpicker_DateSelected"></DatePicker>
            
            <HorizontalStackLayout x:Name="hs">

                <ImageButton  x:Name="goback_btn" Clicked="goback_btn_Clicked" HorizontalOptions="Start"  Source="home.png" MaximumHeightRequest="40" MaximumWidthRequest="30">
                </ImageButton>
            </HorizontalStackLayout>
            <ActivityIndicator IsVisible="True" IsRunning="True" IsEnabled="True" x:Name="actind" ></ActivityIndicator>
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>